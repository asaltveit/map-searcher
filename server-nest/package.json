{
  "name": "map-searcher-api",
  "version": "0.0.1",
  "description": "NestJS backend API for map-searcher",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "PORT=3001 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "dev": "docker compose up -d && PORT=3001 nest start --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "test": "jest --runInBand --no-watchman",
    "test:watch": "jest --watch --no-watchman",
    "test:cov": "jest --coverage --no-watchman",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "studio": "npx prisma studio",
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "services:logs": "docker compose logs -f",
    "openapi:gen": "tsx scripts/generate-openapi.ts",
    "postman:gen": "portman -l docs/openapi.json -c portman/portman-config.yml -s portman/postman-config.json -o postman/collection.json && node scripts/transform-optional-fields.js",
    "postman:push": "tsx scripts/push-to-postman.ts",
    "postman:sync:envs": "tsx scripts/sync-environments.ts",
    "postman:sync": "npm run openapi:gen && npm run postman:gen && npm run postman:push && npm run postman:sync:envs"
  },
  "dependencies": {
    "@bull-board/api": "^6.17.0",
    "@bull-board/express": "^6.17.0",
    "@bull-board/nestjs": "^6.17.0",
    "@letta-ai/letta-client": "^1.7.8",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.13",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/swagger": "^11.2.6",
    "@prisma/adapter-pg": "7.4.0",
    "@prisma/client": "^7.4.0",
    "@sentry/nestjs": "^10.29.0",
    "@sentry/profiling-node": "^10.29.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.68.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.4",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "8.18.0",
    "prisma": "^7.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^13.0.0",
    "weave": "^0.11.0"
  },
  "devDependencies": {
    "@apideck/portman": "^1.26.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "8.16.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^30.0.0",
    "newman": "^6.2.2",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.49.0"
  }
}
