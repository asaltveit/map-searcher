# Server
PORT=3000
NODE_ENV=development

# Database (PostgreSQL/Neon)
DATABASE_URL=postgresql://user:password@localhost:5432/map_searcher

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Redis (for BullMQ and Research storage)
REDIS_URL=redis://localhost:6379
# Or use individual vars:
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=
# REDIS_USE_TLS=false

# Letta AI
LETTA_API_KEY=your-letta-api-key

# Sentry Error Tracking (optional)
SENTRY_DSN=

# W&B Weave Tracing (optional)
WANDB_API_KEY=

# Bull Board Dashboard
ENABLE_BULL_BOARD=true
BULL_BOARD_USER=admin
BULL_BOARD_PASS=admin

# Multi-user mode (optional)
ENABLE_HYBRID_MULTI_USER=false

# Postman API (for collection sync)
POSTMAN_API_KEY=
POSTMAN_WORKSPACE_ID=
POSTMAN_COLLECTION_ID=
